Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
URxvt*transparent: false
!URxvt*blurRadius: 10
URxvt*fading: 15
URxvt*shading: 10
URxvt*scrollBar: False
URxvt*saveLines: 1000
rxvt*modifier: alt
URxvt.font: xft:Monaco For Powerline:regular:size=9
URxvt.letterSpace: -1
!URxvt.font: xft:Clean:size=9 

*foreground:	#ada07e
*background:	#1f201d

!black
